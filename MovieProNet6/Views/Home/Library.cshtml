@{
    ViewData["Title"] = "Library";
}
<h1 class="mb-3">@ViewData["Title"]</h1>

<div class="row">
    <div class="col-12 col-sm-4">
        <div class="row">
            <h2>TMDB Movies</h2>
         </div>
        <div class="row">
         <select name="MovieTMDBValues" id="MovieTMDBValues" multiple class="form-select"></select>
        </div>
    </div>
    <div class="col-12 col-sm-4">   
        
            <button onclick="AddMovie()" name="Add" type="button" class="btn btn-primary col-12 mt-3">Add</button>   
            <button onclick="DeleteMovie()" name="Delete" type="button" class="btn btn-danger col-12 mt-3">Delete</button>        
    
        
     </div>
    <div class="col-12 col-sm-4">
        <div class="row">
            <h2>All Collection Library</h2>
        </div>
        <div class="row">
         <select name="MovieLibraryValues" id="MovieLibraryValues" multiple class="form-select"></select>
        </div>
    </div>
</div>